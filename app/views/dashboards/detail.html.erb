  <body>
    <h4>Question 1: Does this logo make you think of pizza? </h4>
    <div id="AgeGender" style="width: 700px; height: 300px;"></div>
    <div id="Income" style="width: 700px; height: 300px;"></div>
  </body>
  
    <script type="text/javascript" src="https://www.google.com/jsapi"></script>
    <script type="text/javascript">
    
    google.load("visualization", "1.1", {packages:["bar"]});
      google.setOnLoadCallback(drawAgeGenderChart);
      
      function drawAgeGenderChart() {
        var jsonData = google.visualization.arrayToDataTable([
          ['Age', 'Male (%)', 'Female (%)'],
          ['0-18', 35, 40],
          ['19-25', 57, 65],
          ['26-40', 41, 47],
          ['41+', 12, 15]
        ]);

        var options = {
          chart: {
            title: 'Age and Gender',
            subtitle: 'Last Updated: August 22, 2015',
          },
          bars: 'vertical' // Required for Material Bar Charts.
        };

        var chart = new google.charts.Bar(document.getElementById('AgeGender'));

        chart.draw(jsonData, options);
      }
      
        google.setOnLoadCallback(drawIncomeChart);
      
      function drawIncomeChart() {
        var data = google.visualization.arrayToDataTable([
          ['Income', 'Users (%)'],
          ['$0-50,000', 31],
          ['$50,001-120,000', 52],
          ['$120,001+', 17],
        ]);

        var options = {
          chart: {
            title: 'Annual Household Income',
            subtitle: 'Last Updated: August 22, 2015',
          },
          bars: 'vertical' // Required for Material Bar Charts.
        };

        var chart = new google.charts.Bar(document.getElementById('Income'));

        chart.draw(data, options);
      }
      
      
    </script>